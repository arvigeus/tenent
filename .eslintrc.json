{
  "extends": ["permissive", "permissive/nextjs"],
  "settings": {
    "react": {
      "version": "detect"
    },
    "import/core-modules": ["react-testing-library", "logger"],
    "rules": {
      "react-hooks/exhaustive-deps": [
        "warn",
        {
          "additionalHooks": "useLoadFileSystemCallback"
        }
      ]
    }
  }
}
